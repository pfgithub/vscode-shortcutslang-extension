{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Shortcutslang",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.flowcut",
				"match": "\\b[A-Za-z0-9@.]+\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.flowcut",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.flowcut",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.shortcutslang"
}